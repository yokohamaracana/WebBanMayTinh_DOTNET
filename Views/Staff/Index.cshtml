@{
    Layout = "_LayoutAdmin";
    var staff = ViewData["staff"] as List<staff>;
    var alertStoreStaff = ViewData["alertStoreStaff"];
    
}
  @if(Convert.ToInt32(alertStoreStaff) == 0)
  {
    <div class="alert alert-success" role="alert">
       Thêm thành công
    </div>
  }
<main>
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            DataTable Example
        </div>
        <div class="card-body">
            <table id="datatablesSimple">
                <thead>
                    <tr>
                        <th>Tài khoản</th>
                        <th>Mật khẩu</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Tài khoản</th>
                        <th>Mật khẩu</th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var item in staff)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td>@item.Password</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    </div>
</main>