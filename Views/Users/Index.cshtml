@{
    Layout = "_LayoutAdmin";
    var users = ViewData["users"] as List<User>;
}
<main>
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            DataTable Example
        </div>
        <div class="card-body">
            <table id="datatablesSimple">
                <thead>
                    <tr>
                        <th>Email</th>
                        <th>Phone Number</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Email</th>
                        <th>Phone Number</th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var item in users)
                    {
                    <tr>
                        <td>@item.Email</td>
                        <td>@item.PhoneNumber</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    </div>
</main>